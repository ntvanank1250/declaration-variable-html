    <button name="button" type="button" onclick="bai2()">x co chia het cho y khong?</button>
    <script>function bai2() {
        let x = prompt("nhap so x:", '')
        let y = prompt("nhap so y:", '')
        x = parseInt(x)
        y = parseInt(y)
        if
            (x % y == 0) {
           
            alert(" x chia het cho y")
        }
        else {
            
            alert("x khong chia het cho y")
        }
    }
    </script>

